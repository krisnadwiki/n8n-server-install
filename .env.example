# n8n Configuration
# Copy this file to .env and update the values

# Basic Authentication
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=changeme

# Host Configuration
N8N_HOST=0.0.0.0
N8N_PROTOCOL=http
WEBHOOK_URL=http://your-domain.com/

# Timezone
GENERIC_TIMEZONE=UTC
